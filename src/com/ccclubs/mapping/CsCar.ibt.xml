	<!-- 获取所有车辆 -->
	<select id="getCsCarList" resultMap="CsCar">
		select cs_car$suffix$.* from cs_car$suffix$
		
		<!-- 自动定义扩展sql连接join,在参数map里放完整的join条件 -->
		<isNotEmpty property="join">
		    $join$
		</isNotEmpty>
		<!-- 多对一外链接[车型]查询-->
		<isNotNull property="CscModel"><!-- 调用一下CscModel，把CsCarModel指向CscModel --></isNotNull>
		<isNotNull property="CsCarModel">
			<isNull property="CsCarModel.on">
				left join cs_car_model cs_car_model$CsCarModel.suffix$ on cs_car$suffix$.csc_model=cs_car_model$CsCarModel.suffix$.cscm_id
			</isNull>
			<isNotNull property="CsCarModel.on">
				left join cs_car_model cs_car_model$CsCarModel.suffix$ on cs_car$suffix$.$CsCarModel.on$=cs_car_model$CsCarModel.suffix$.cscm_id			
			</isNotNull>
		</isNotNull>
		<!-- 多对一外链接[网点]查询-->
		<isNotNull property="CscOutlets"><!-- 调用一下CscOutlets，把CsOutlets指向CscOutlets --></isNotNull>
		<isNotNull property="CsOutlets">
			<isNull property="CsOutlets.on">
				left join cs_outlets cs_outlets$CsOutlets.suffix$ on cs_car$suffix$.csc_outlets=cs_outlets$CsOutlets.suffix$.cso_id
			</isNull>
			<isNotNull property="CsOutlets.on">
				left join cs_outlets cs_outlets$CsOutlets.suffix$ on cs_car$suffix$.$CsOutlets.on$=cs_outlets$CsOutlets.suffix$.cso_id			
			</isNotNull>
		</isNotNull>
		<!-- 多对一外链接[供应商]查询-->
		<isNotNull property="CscProvid"><!-- 调用一下CscProvid，把CsProvid指向CscProvid --></isNotNull>
		<isNotNull property="CsProvid">
			<isNull property="CsProvid.on">
				left join cs_provid cs_provid$CsProvid.suffix$ on cs_car$suffix$.csc_provid=cs_provid$CsProvid.suffix$.csp_id
			</isNull>
			<isNotNull property="CsProvid.on">
				left join cs_provid cs_provid$CsProvid.suffix$ on cs_car$suffix$.$CsProvid.on$=cs_provid$CsProvid.suffix$.csp_id			
			</isNotNull>
		</isNotNull>
				
		where 1=1 
		and cs_car$suffix$.csc_status!=-1
		<!-- 多对一外链接[车型]查询条件sql片段 -->
		<isNotNull property="CscModel"><!-- 调用一下CscModel，把CsCarModel指向CscModel --></isNotNull>
		<isNotNull property="CsCarModel">
			<include refid="CsCarModel_part.cs_car_model_join"/>
		</isNotNull>
		<!-- 多对一外链接[网点]查询条件sql片段 -->
		<isNotNull property="CscOutlets"><!-- 调用一下CscOutlets，把CsOutlets指向CscOutlets --></isNotNull>
		<isNotNull property="CsOutlets">
			<include refid="CsOutlets_part.cs_outlets_join"/>
		</isNotNull>
		<!-- 多对一外链接[供应商]查询条件sql片段 -->
		<isNotNull property="CscProvid"><!-- 调用一下CscProvid，把CsProvid指向CscProvid --></isNotNull>
		<isNotNull property="CsProvid">
			<include refid="CsProvid_part.cs_provid_join"/>
		</isNotNull>
		
		<!-- 动态条件sql片段 -->
		<include refid="CsCar_part.cs_car_dynamic"/>
		
		<!-- 自动定义扩展sql条件definex,在参数map里放完整的sql条件,如 tc_status>=0 -->
		<isNotEmpty property="definex" prepend="and">
		    $definex$
		</isNotEmpty>
		
		<!-- 页面传递的降序字段 -->
		<isNotEmpty property="desc">
		   order by $desc$ desc
		</isNotEmpty>
		<!-- 页面传递的升序字段 -->
		<isNotEmpty property="asc">
		   order by  $asc$ asc
		</isNotEmpty>
		<!-- 自定排序字段 -->
		<isNull property="asc">
			<isNull property="desc">order by csc_id desc
			</isNull>
		</isNull>
		
		
		<isNotEmpty property="limit">
			limit #limit# 
		</isNotEmpty>
		<isEmpty property="limit">
			limit 255
		</isEmpty>
		
	</select>
	
	
	

